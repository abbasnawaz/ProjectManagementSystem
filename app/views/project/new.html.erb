<div class="container">
    <%= form_with model:@project, method: :post do |f| %>
        <div class="form-group">
            <%= f.label :project_name %>
            <%= f.text_field :project_name, class: "form-control" %>
        </div>
        <div class="form-group">
            <%= f.label :starting_date %>
            <%= f.date_field :starting_date, class: "form-control" %>
        </div>
        <div class="form-group">
            <%= f.label :ending_date %>
            <%= f.date_field :ending_date, class: "form-control" %>
        </div>
        <div class="form-group">
            <%= f.submit "Create Project", class: "btn btn-primary" %>
        </div>
    <% end %>
</div>