
<% if flash[:error]%>
  <div class = "container">
    <div class="alert alert-danger" role="alert">
      <%= flash[:error]%>
    </div>
  </div>
<% end %>

<% if flash[:notice]%>
  <div class = "container">
    <div class="alert alert-info" role="alert">
      <%= flash[:notice]%>
    </div>
  </div>
<% end %>


<% if flash[:warning]%>
  <div class = "container">
    <div class="alert alert-info" role="alert">
      <%= flash[:warning]%>
    </div>
  </div>
<% end %>

<% if flash[:success]%>
  <div class = "container">
    <div class="alert alert-info" role="alert">
      <%= flash[:success]%>
    </div>
  </div>
<% end %>    
